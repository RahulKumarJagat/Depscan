{
  "name": "depscan",
  "displayName": "Depscan",
  "description": "A dependency scanner for Python and Node.js projects",
  "version": "0.0.1",
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/RahulKumarJagat/Depscan"
  },
  "engines": {
    "vscode": "^1.95.0"
   
  },
  
  "categories": [
    "Other"
  ],

  "activationEvents": [
    "onCommand : depscan.scanDependencies"
],

  "main": "./extension.js",
  "contributes": {
    "commands": [
        {
            "command": "depscan.scanDependencies",
            "title": "Scan Dependencies"
        }
    ]
}
  ,



  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0"
  },


  "dependencies": {
    "json5": "^2.2.3"
  }
}
